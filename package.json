{
  "name": "marko-test-setup",
  "description": "The default Marko starter app",
  "version": "1.0.0",
  "dependencies": {
    "marko": "^5.33.5"
  },
  "devDependencies": {
    "@marko/jest": "^6.0.0",
    "@marko/run": "^0.4.3",
    "@marko/testing-library": "^6.2.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "prettier-plugin-marko": "^3.1.2"
  },
  "private": true,
  "scripts": {
    "dev": "marko-run",
    "build": "marko-run build",
    "preview": "marko-run preview",
    "start": "node --enable-source-maps ./dist/index.mjs",
    "test": "jest",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json)\"",
    "check-format": "npm run prettier -- --list-different",
    "format": "npm run prettier -- --write",
    "validate": "npm run check-format && npm run build"
  }
}
